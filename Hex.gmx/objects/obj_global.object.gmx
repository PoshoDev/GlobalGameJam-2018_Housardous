        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Actually do the Actions
if (current &gt;= 4)
    alarm[6] = 5;
else if (global.players-1 &gt;= global.organization[current])
{
    pid = global.player_id[global.organization[current]];    
    
    if (pid.idee == global.orders[pid.idee, 0]) // It chose itself.
    {
        pid.state = "walk";
        tile_target(pid, global.orders[pid.index, 1]);
        
        //plop();
    }
    else
    {
        //boop();
        
        pod = global.player_id[global.orders[pid.index, 0]];
        
        pod.state = "ghost";
        pod.possessor = pid.idee;
        pod.face_current = pid.idee;
        witch_sound(pod.index);
        
        pod.ghost = true;
        pod.ghost_id = pid.idee;
        
        tile_target(pod, global.orders[pid.index, 1]);   
    }
    
}
else if (++current &lt; 4)
    alarm[5] = 5;
</string>
          </argument>
        </arguments>
      </action>
