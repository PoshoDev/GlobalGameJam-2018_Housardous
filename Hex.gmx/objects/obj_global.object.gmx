            <kind>1</kind>
            <string>/// RANDOMIZE &amp; ORGANIZE ACTIONS
global.organization[0] = 0;
global.organization[1] = 1;
global.organization[2] = 2;
global.organization[3] = 3;

var i, j, k;
var size = 4;
for (i = 0; i &lt; size; i += 1)
{
    j = irandom_range(i, size - 1)
    if (i != j)
    {
        k = global.organization[i]
        global.organization[i] = global.organization[j]
        global.organization[j] = k
    }
}

current = 0;

audio_play_sound(msc_ruckus, 0, false);

alarm[5] = 5;
</string>
